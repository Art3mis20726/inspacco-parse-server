{
  "name": "parse-server-ts",
  "version": "1.0.0",
  "description": "Parse server using typescript",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/inspacco/inspacco-parse-server"
  },
  "engines": {
    "node": ">=14.16.0 <=16.14.2"
  },
  "author": "Prophandy Technologies Pvt. Ltd.",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf dist/",
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
    "build-ts": "npm run clean && tsc",
    "build": "npm run build-ts && npm run lint",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "npm run build-ts && nodemon --inspect dist/server.js",
    "serve": "npm run build-ts && node dist/server.js",
    "start": "node dist/server.js",
    "test": "jest --forceExit --coverage --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.150.0",
    "@types/babel__core": "7.1.18",
    "@types/babel__traverse": "7.14.2",
    "@types/chai": "4.2.14",
    "@types/concurrently": "5.2.1",
    "@types/eslint": "7.2.6",
    "@types/express": "4.17.11",
    "@types/fs-capacitor": "2.0.0",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/moment-timezone": "^0.5.30",
    "@types/mongodb": "^3.6.19",
    "@types/node": "14.14.25",
    "@types/nodemailer": "6.4.0",
    "@types/parse": "^2.18.5",
    "@types/pug": "^2.0.5",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "archiver": "6.0.2",
    "async-lock": "^1.4.1",
    "aws-sdk": "^2.1499.0",
    "cache-manager": "^5.6.1",
    "chai": "4.3.0",
    "concurrently": "5.3.0",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "eslint": "7.19.0",
    "exceljs": "^4.4.0",
    "expo-server-sdk": "^3.6.0",
    "express": "^4.18.2",
    "fast-csv": "^4.3.6",
    "fcm-node": "^1.6.1",
    "file-type": "16.5.1",
    "follow-redirects": "^1.15.6",
    "html-pdf": "^3.0.1",
    "jest": "26.6.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongoose": "5.13.14",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.22",
    "npm-force-resolutions": "0.0.10",
    "p-limit": "^3.1.0",
    "parse-server": "^4.10.4",
    "prettier": "^2.2.1",
    "pug": "^3.0.2",
    "request": "^2.88.2",
    "rollbar": "^2.26.4",
    "sharp": "0.32.6",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "typescript": "4.6.3",
    "winston": "3.3.3"
  },
  "resolutions": {
    "axios": "0.21.1",
    "netmask": "2.0.1",
    "lodash": "4.17.21",
    "@types/bson": "4.0.3"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/async-lock": "^1.4.2",
    "rimraf": "^5.0.7"
  }
}
